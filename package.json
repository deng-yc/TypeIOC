    "coveralls": "cat test/coverage/lcov.info | coveralls",
    "codeclimate": "cat test/coverage/lcov.info | CODECLIMATE_REPO_TOKEN=4d695ca8fbc1be6c4f20509a918f006eb3ac84c871364cb935ac0f75ec852e01  codeclimate"
